{
  "name": "@zwave-js/config",
  "version": "10.15.0",
  "description": "zwave-js: configuration files",
  "publishConfig": {
    "access": "public"
  },
  "main": "build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./package.json": "./package.json",
    "./safe": "./build/index_safe.js"
  },
  "types": "build/index.d.ts",
  "typesVersions": {
    "*": {
      "safe": [
        "build/index_safe.d.ts"
      ]
    }
  },
  "keywords": [],
  "files": [
    "build/**/*.{js,d.ts,map}",
    "config/"
  ],
  "author": {
    "name": "AlCalzone",
    "email": "d.griesel@gmx.net"
  },
  "license": "MIT",
  "homepage": "https://github.com/AlCalzone/node-zwave-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlCalzone/node-zwave-js.git"
  },
  "bugs": {
    "url": "https://github.com/AlCalzone/node-zwave-js/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/AlCalzone/"
  },
  "engines": {
    "node": ">=14.13.0 <15 || >= 16 <16.9.0 || >16.9.0"
  },
  "scripts": {
    "codegen": "yarn ts maintenance/codegen.ts",
    "build": "tsc -b tsconfig.build.json --pretty",
    "clean": "del-cli build/ \"*.tsbuildinfo\"",
    "extract-api": "yarn api-extractor run",
    "lint:zwave": "yarn ts maintenance/lintConfigFiles.ts",
    "ts": "node -r esbuild-register",
    "lint:ts": "eslint --ext .ts --rule \"prettier/prettier: off\" \"src/**/*.ts\"",
    "lint:prettier": "prettier -c \"src/**/*.ts\" \"config/**/*.json\"",
    "lint:ts:fix": "yarn run lint:ts --fix",
    "lint:prettier:fix": "yarn run lint:prettier -w",
    "test:ts": "ava",
    "test:dirty": "node -r ../../maintenance/esbuild-register.js ../maintenance/src/resolveDirtyTests.ts --run"
  },
  "dependencies": {
    "@zwave-js/core": "workspace:*",
    "@zwave-js/shared": "workspace:*",
    "alcalzone-shared": "^4.0.8",
    "ansi-colors": "^4.1.3",
    "fs-extra": "^10.1.0",
    "json-logic-js": "^2.0.2",
    "json5": "^2.2.2",
    "semver": "^7.3.7",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@esm2cjs/got": "^12.5.3",
    "@microsoft/api-extractor": "*",
    "@types/fs-extra": "^9.0.13",
    "@types/js-levenshtein": "^1.1.1",
    "@types/json-logic-js": "^2.0.1",
    "@types/node": "^14.18.36",
    "@types/pegjs": "^0.10.3",
    "@types/semver": "^7.3.12",
    "@types/sinon": "^10.0.13",
    "@types/xml2js": "^0.4.11",
    "@types/yargs": "^17.0.12",
    "@zwave-js/maintenance": "workspace:*",
    "ava": "^4.3.3",
    "comment-json": "^4.2.3",
    "del-cli": "^5.0.0",
    "esbuild": "0.15.7",
    "esbuild-register": "^3.4.2",
    "js-levenshtein": "^1.1.6",
    "pegjs": "^0.10.0",
    "prettier": "^2.8.4",
    "sinon": "^14.0.0",
    "ts-pegjs": "^0.3.1",
    "typescript": "4.9.5",
    "xml2js": "^0.5.0",
    "yargs": "^17.5.1"
  }
}
